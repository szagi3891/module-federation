{
  "name": "players",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "build": "rm -Rf dist && webpack --progress --config ./webpack.config.js",
    "build:server": "webpack --progress --config webpack.server.config.js",
    "build:client": "webpack --progress --config webpack.client.config.js",
